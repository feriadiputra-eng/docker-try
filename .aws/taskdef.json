{
    "containerDefinitions": [
        {
            "essential": true,
            "image": "793603127187.dkr.ecr.us-east-1.amazonaws.com/nodejs:latest",
            "name": "nodejs",
            "portMappings": [
                {
                    "hostPort": 5000,
                    "containerPort": 5000,
                    "protocol": "tcp"
                }
            ]
        },
        {
            "essential": true,
            "image": "793603127187.dkr.ecr.us-east-1.amazonaws.com/react-movie:latest",
            "name": "react_movie",
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "memory": "4096",
    "cpu": "2048",
    "networkMode": "awsvpc",
    "family": "node_react",
    "executionRoleArn": "arn:aws:iam::793603127187:role/LabRole",
    "requiresCompatibilities": ["FARGATE"]
}