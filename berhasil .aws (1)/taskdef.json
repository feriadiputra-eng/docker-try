{
    "containerDefinitions": [
        {
            "essential": true,
            "image": "878223349427.dkr.ecr.us-east-1.amazonaws.com/nodejs-app:latest",
            "name": "nodejs",
            "portMappings": [
                {
                    "hostPort": 3000,
                    "containerPort": 3000,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "memory": "3072",
    "cpu": "1024",
    "networkMode": "awsvpc",
    "family": "nodejs",
    "executionRoleArn": "arn:aws:iam::878223349427:role/ecsTaskExecutionRole",
    "requiresCompatibilities": ["FARGATE"]
}